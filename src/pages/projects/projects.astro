    <script is:inline>
        // Tab switching functionality
        const ongoingTab = document.getElementById('ongoingTab');
        const completedTab = document.getElementById('completedTab');
        const ongoingProjects = document.getElementById('ongoingProjects');
        const completedProjects = document.getElementById('completedProjects');
        const searchInput = document.getElementById('searchInput');
        const noResults = document.getElementById('noResults');
        const loadMoreBtn = document.getElementById('loadMoreBtn');
        const hiddenProjects = document.getElementById('hiddenProjects');
        const completedProjectsGrid = document.getElementById('completedProjectsGrid');

        ongoingTab.addEventListener('click', () => {
            ongoingTab.classList.add('tab-active');
            ongoingTab.classList.remove('bg-gray-900');
            completedTab.classList.remove('tab-active');
            completedTab.classList.add('bg-gray-900');
            ongoingProjects.classList.remove('hidden');
            completedProjects.classList.add('hidden');
            searchInput.value = '';
            filterProjects('');
        });

        completedTab.addEventListener('click', () => {
            completedTab.classList.add('tab-active');
            completedTab.classList.remove('bg-gray-900');
            ongoingTab.classList.remove('tab-active');
            ongoingTab.classList.add('bg-gray-900');
            completedProjects.classList.remove('hidden');
            ongoingProjects.classList.add('hidden');
            searchInput.value = '';
            filterProjects('');
        });

        // Search functionality
        searchInput.addEventListener('input', (e) => {
            const searchTerm = e.target.value.toLowerCase();
            filterProjects(searchTerm);
        });

        function filterProjects(searchTerm) {
            const activeTab = ongoingTab.classList.contains('tab-active') ? ongoingProjects : completedProjects;

            // Get all project cards from both visible and hidden sections
            let allProjects = [];
            if (activeTab === ongoingProjects) {
                allProjects = Array.from(activeTab.querySelectorAll('.project-card'));
            } else {
                allProjects = [
                    ...Array.from(completedProjectsGrid.querySelectorAll('.project-card')),
                    ...Array.from(hiddenProjects.querySelectorAll('.project-card'))
                ];
            }

            let visibleCount = 0;

            allProjects.forEach(project => {
                const title = project.querySelector('h3').textContent.toLowerCase();
                const description = project.querySelector('p').textContent.toLowerCase();

                if (title.includes(searchTerm) || description.includes(searchTerm)) {
                    project.classList.remove('hidden');
                    visibleCount++;

                    // If it's a completed project and matches search, move it to the visible grid
                    if (activeTab === completedProjects && project.parentElement.parentElement === hiddenProjects) {
                        completedProjectsGrid.appendChild(project.cloneNode(true));
                        project.classList.add('hidden'); // Hide in original location
                    }
                } else {
                    project.classList.add('hidden');
                }
            });

            if (visibleCount === 0) {
                noResults.classList.remove('hidden');
                loadMoreBtn.parentElement.classList.add('hidden');
            } else {
                noResults.classList.add('hidden');

                // Only show load more button if there are hidden projects and we're on completed tab
                if (activeTab === completedProjects && searchTerm === '') {
                    loadMoreBtn.parentElement.classList.remove('hidden');
                } else if (activeTab === completedProjects && searchTerm !== '') {
                    loadMoreBtn.parentElement.classList.add('hidden');
                }
            }
        }

        // Load more functionality - completely revised
        loadMoreBtn.addEventListener('click', () => {
            // Get all projects from hidden section
            const hiddenProjectCards = hiddenProjects.querySelectorAll('.project-card');

            // Clone all hidden projects and append them to the visible grid
            hiddenProjectCards.forEach(card => {
                completedProjectsGrid.appendChild(card.cloneNode(true));
            });

            // Hide the load more button since all projects are now visible
            loadMoreBtn.parentElement.classList.add('hidden');

            // Hide the hidden projects container
            hiddenProjects.classList.add('hidden');
        });
    </script>

	
