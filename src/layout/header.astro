---

const { pathname } = Astro.url;

let pageTitle = 'Home';

if (pathname === '/') {
  pageTitle = 'Home | TechFlame';
} else if (pathname === '/about-us') {
  pageTitle = 'About Us | TechFlame';
} else if (pathname  === '/about-us/history') {
  pageTitle = 'History | TechFlame';
}else if (pathname === '/about-us/firefighters') {
  pageTitle = 'FireFighters | TechFlame';
}
else if (pathname === '/services') {
  pageTitle = 'Services | TechFlame';
}
else if (pathname === '/services/service-detail') {
  pageTitle = 'Services Details | TechFlame';
}
else if (pathname === '/products') {
  pageTitle = 'Products | TechFlame';
}
else if (pathname === '/credentials') {
  pageTitle = 'Credentials | TechFlame';
}
else if (pathname === '/testimonials') {
  pageTitle = 'Testimonials | TechFlame';
}
else if (pathname === '/career') {
  pageTitle = 'Career | TechFlame';
}
else if (pathname === '/contact') {
  pageTitle = 'Contact | TechFlame';
}
else if (pathname === '/blog') {
  pageTitle = 'Blog | TechFlame';
}
else if (pathname === '/page/gallery') {
  pageTitle = 'Gallery | TechFlame';
}
else if (pathname === '/page/faq') {
  pageTitle = 'FAQ | TechFlame';
}


